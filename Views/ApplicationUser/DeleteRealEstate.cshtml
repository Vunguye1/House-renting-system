@model Realestate

<div class="container">
    <h2>Delete real estate</h2>

    <h3>Are you sure you want to delete this real estate?</h3>
    <div>
        <h4>Item</h4>
        <hr />
        <dl class="row">
            <dt class="col-sm-2">Real estate Id</dt>
            <dd class="col-sm-10">@Model.RealestateId</dd>

            <dt class="col-sm-2">Name</dt>
            <dd class="col-sm-10">@Model.Name</dd>

            <dt class="col-sm-2">Price</dt>
            <dd class="col-sm-10">@Model.Price</dd>

            <dt class="col-sm-2">Description</dt>
            <dd class="col-sm-10">@Model.Description</dd>

            <dt class="col-sm-2">Image URL</dt>
            <dd class="col-sm-10">@Model.imageurl</dd>
        </dl>

        <form asp-action="DeleteConfirmed" asp-route-id="@Model.RealestateId" method="post">
            <button type="submit" class="btn btn-danger">Delete</button>
            <a asp-controller="Realestate" asp-action="GeneralGrid" class="btn btn-secondary">Cancel</a>
        </form>
    </div>
</div>
