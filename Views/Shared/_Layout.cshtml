<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["CurrentViewName"] - Your best booking services</title>
    <link rel="stylesheet" href="/css/index.css">
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css" rel="stylesheet">


    <script src="~/lib/jquery/jquery.js"></script>
    <script src="~/lib/popper.js/umd/popper.min.js"></script>
    <script src="~/lib/bootstrap/js/bootstrap.js"></script>

    <script src="~/lib/jquery-validate/jquery.validate.js"></script>
    <script src="~/lib/jquery-validation-unobtrusive/jquery.validate.unobtrusive.js"></script>

</head>
<body>
    <header>
        <div class="d-flex justify-content-between bg-white border-bottom navbardiv">

                <div>
                    <a class="navbar-brand text-black ms-3" asp-area="" asp-controller="Home" asp-action="Index">Bolig saken</a>
                    <a class="navbar-brand text-black" asp-controller="Realestate" asp-action="GeneralGrid">Book</a>
                </div>
                

                <div>
                    <ul>
                        <partial name="_LoginPartial" />
                    </ul>
                </div>
            </div>
        
    </header>
    <main role="main">
        @RenderBody()
        @RenderSection("scripts", required: false)
    </main>
    
</body>
</html>

