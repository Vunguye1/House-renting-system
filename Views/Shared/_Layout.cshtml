<!--main structure-->


<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["CurrentViewName"] - Your best booking services</title>
    <link rel="stylesheet" href="/css/index.css">
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css" rel="stylesheet">


    <script src="~/lib/jquery/jquery.js"></script>
    <script src="~/lib/popper.js/umd/popper.min.js"></script>
    <script src="~/lib/bootstrap/js/bootstrap.js"></script>

    <script src="~/lib/jquery-validate/jquery.validate.js"></script>
    <script src="~/lib/jquery-validation-unobtrusive/jquery.validate.unobtrusive.js"></script>

</head>
<body>
    <header>
        <!--header contains nav bar-->

        <div class="d-flex justify-content-between border-bottom-0 navbardiv fixed-top">

            <div class="d-flex align-items-center">
                <a class="navbar-brand text-black ms-3" asp-area="" asp-controller="Home" asp-action="Index"><img src="/img/icons/house-heart-fill.svg" height="40" width="40" /></a>
                <a class="navbar-brand text-black" asp-controller="Realestate" asp-action="GeneralGrid">Book</a>
            </div>


            <div class="d-flex align-items-center">
                <ul class="d-flex align-items-center">
                    <partial name="_LoginPartial" />
                </ul>
            </div>
        </div>

    </header>

    <!--contents in different views go here-->

    <main role="main" id="maincontainer">
        @RenderBody()
        @RenderSection("scripts", required: false)
    </main>

</body>
</html>

